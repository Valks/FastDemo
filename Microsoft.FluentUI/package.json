    "@microsoft/fast-foundation": "latest",
    "lodash-es": "latest",
    "tslib": "latest"
  },
  "devDependencies": {
    "http-server": "latest",
    "rimraf": "latest",
    "ts-loader": "latest",
    "typescript": "latest",
